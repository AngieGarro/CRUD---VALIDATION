@page
@model WebApp_ExamenPruebaFinal.Pages.LoginModel
@{
}
<script src="~/js/controlactions.js"></script>
<script src="~/js/pages/login.js"></script>

<h1>Iniciar Sesión</h1>
<br />

<form>
	<div class="form-group">
		<label for="exampleInputEmail1" class="form-label mt-4">Email</label>
		<input type="email" class="form-control" id="txt_Email" placeholder="Ingrese su Email">
	</div>

	<div class="form-group">
		<label for="exampleInputEmail1" class="form-label mt-4">Contraseña</label>
		<input type="password" class="form-control" id="txt_Password" placeholder="Ingrese su Contraseña">
	</div>

	<button type="button" id="LoginUser" class="btn btn-outline-success">Ingresar</button>
	<br />
	<br />
	<li class="breadcrumb-item"><a asp-area="" asp-page="/RestartPassword">Olvidó su contraseña?</a></li>
</form>